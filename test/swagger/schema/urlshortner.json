        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "query",
            "name": "projection",
            "description": "Pet object that needs to be added to the store",
            "required": true,
            "type": "string",
            "default":"FULL",
            "enum": [
              "ANALYTICS_CLICKS",
              "ANALYTICS_TOP_STRINGS",
              "FULL"
            ]
          },
          {
            "in": "query",
            "name": "shortUrl",
            "description": "Pet object that needs to be added to the store",
            "default":"http://goo.gl/xKbRu3",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {"description": "Url","schema":{"$ref": "#/definitions/Url"}},
          "400": { "description": "Invalid Value", "schema": {"$ref": "#/definitions/ApiErrorResponse"}},
