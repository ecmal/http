          "404": { "description": "Not Found", "schema": {"$ref": "#/definitions/ApiErrorResponse"}}
        },
        "security": [
          {
            "api_auth": ["https://www.googleapis.com/auth/urlshortener"],
            "api_key": []
          }
        ]
      },
